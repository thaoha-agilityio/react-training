{
  "name": "big-practice",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "lint:eslint": "eslint --ignore-path .eslintignore . --ext .js,.jsx,.ts,.tsx",
    "fix:eslint": "eslint --fix --ignore-path .eslintignore . --ext .js,.jsx,.ts,.tsx",
    "jest": "jest --passWithNoTests",
    "jest:watch": "jest --watch",
    "prepare": "cd ../ && husky install",
    "pre-commit-lint": "pnpm lint-staged",
    "pre-push-lint": "rm -rf dist/ && vite build"
  },
  "dependencies": {
    "@chakra-ui/anatomy": "^2.2.1",
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.1",
    "@chakra-ui/styled-system": "^2.6.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@tanstack/react-query": "^4.35.0",
    "axios": "1.4.0",
    "framer-motion": "^10.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-compare": "^3.2.2",
    "react-hook-form": "^7.47.0",
    "react-query": "^3.39.3",
    "react-router-dom": "6.12.1",
    "vite-plugin-imagemin": "^0.6.1",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-conventional": "17.6.7",
    "@testing-library/dom": "8.20.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.4.0",
    "@types/node": "20.3.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/testing-library__jest-dom": "5.14.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "8.0.3",
    "jest": "29.1.2",
    "jest-environment-jsdom": "28.1.3",
    "jest-transform-stub": "2.0.0",
    "lint-staged": "13.2.2",
    "ts-jest": "28.0.7",
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vite-plugin-environment": "1.1.3",
    "vite-plugin-sitemap": "^0.4.2",
    "vite-tsconfig-paths": "4.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint src/*.{js,jsx,ts,tsx} --fix-dry-run --no-error-on-unmatched-pattern",
      "pnpm run jest"
    ]
  }
}
